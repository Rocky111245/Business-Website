//import React from 'react';
import './App.css';
import products from './images_description.json';

function App() {
    return(
        <div className='App'>
            <div className='products'>
                {products.map(post => (
                    <div className='product_container'>
                        <img src={post.productlink} alt={post.name}/>
                        <h3>{post.name}</h3>
                        <h4>{post.description}</h4>
                    </div>
                ))}
            </div>
        </div>
    )
}

export default App;



/* Styles for the main container */
.App {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
}

/* Styles for the products container */
.products {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
    grid-gap: 10px;
    margin: 0 auto;
}

/* Styles for each product container */
.product_container {
    text-align: center;
    position: relative;
    padding: 10px;
    border-radius: 5px;
    background-color: #f9f9f9;
    box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.1);
}

/* Styles for the product image */
.product_container img {
    width: 100%;
    height: auto;
    object-fit: cover;
    display: block;
    margin: 0 auto;
    border-radius: 5px;
}

/* Styles for the product name */
.product_container h3 {
    font-size: 1.2rem;
    font-weight: bold;
    margin: 10px 0;
    text-align: center;
}

/* Styles for the product description */
.product_container h4 {
    font-size: 1rem;
    margin: 10px 0;
    text-align: center;
}


